<?xml version="1.0" encoding="UTF-8"?>

<LoginConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">  
  <LoginModules> 
    <LoginModule> 
      <name>AuthenProcessor</name>  
      <location>com.icss.ro.framework.login.chain.process.AuthenProcessor</location>  
      <processType>1</processType>  
      <nodeType>3</nodeType>  
      <priority>10</priority>  
      <enable>true</enable>  
      <depend/> 
    </LoginModule>  
    <LoginModule> 
      <name>UserMenuOnceProcessor</name>  
      <location>com.icss.ro.framework.login.chain.process.UserMenuOnceProcessor</location>  
      <processType>1</processType>  
      <nodeType>3</nodeType>  
      <priority>9</priority>  
      <enable>true</enable>  
      <depend>AuthenProcessor</depend> 
    </LoginModule>  
    <LoginModule> 
      <name>UserMenuByStepProcessor</name>  
      <location>com.icss.ro.framework.login.chain.process.UserMenuByStepProcessor</location>  
      <processType>1</processType>  
      <nodeType>3</nodeType>  
      <priority>9</priority>  
      <enable>true</enable>  
      <depend>AuthenProcessor</depend> 
    </LoginModule>  
    <LoginModule> 
      <name>UersNumControlActivity</name>  
      <location>com.icss.ro.framework.login.chain.activity.UersNumControlActivity</location>  
      <processType>1</processType>  
      <nodeType>1</nodeType>  
      <priority>10</priority>  
      <enable>true</enable>  
      <depend/> 
    </LoginModule>  
    <LoginModule> 
      <name>SecurePolicyActivity</name>  
      <location>com.icss.ro.framework.login.chain.activity.SecurePolicyActivity</location>  
      <processType>1</processType>  
      <nodeType>1</nodeType>  
      <priority>2</priority>  
      <enable>true</enable>  
      <depend>ConcurrentControlActivity</depend> 
    </LoginModule>  
    <LoginModule> 
      <name>GetUserInfoActivity</name>  
      <location>com.icss.ro.framework.login.chain.activity.GetUserInfoActivity</location>  
      <processType>1</processType>  
      <nodeType>1</nodeType>  
      <priority>9</priority>  
      <enable>true</enable> 
    </LoginModule>  
    <LoginModule> 
      <name>BasicAuthenAcivity</name>  
      <location>com.icss.ro.framework.login.chain.activity.BasicAuthenAcivity</location>  
      <processType>1</processType>  
      <nodeType>1</nodeType>  
      <priority>3</priority>  
      <enable>true</enable>  
      <depend/> 
    </LoginModule>  
    <LoginModule> 
      <name>GetSystemActivity</name>  
      <location>com.icss.ro.framework.login.chain.activity.GetSystemActivity</location>  
      <processType>2</processType>  
      <nodeType>1</nodeType>  
      <priority>10</priority>  
      <enable>true</enable>  
      <depend>BasicAuthenAcivity</depend> 
    </LoginModule>  
    <LoginModule> 
      <name>GetApplictionsActivity</name>  
      <location>com.icss.ro.framework.login.chain.activity.GetApplictionsActivity</location>  
      <processType>2</processType>  
      <nodeType>1</nodeType>  
      <priority>9</priority>  
      <enable>true</enable>  
      <depend>GetSystemActivity</depend> 
    </LoginModule>  
    <LoginModule> 
      <name>GetMenuActivity</name>  
      <location>com.icss.ro.framework.login.chain.activity.GetMenuActivity</location>  
      <processType>2</processType>  
      <nodeType>1</nodeType>  
      <priority>8</priority>  
      <enable>true</enable>  
      <depend>GetApplictionsActivity</depend> 
    </LoginModule>  
    <LoginModule> 
      <name>OpenPortalCondition</name>  
      <location>com.icss.ro.framework.login.chain.condition.OpenPortalCondition</location>  
      <processType>1</processType>  
      <nodeType>2</nodeType>  
      <priority>8</priority>  
      <enable>true</enable>  
      <depend>ConcurrentControlActivity</depend> 
    </LoginModule>  
    <LoginModule> 
      <name>LoginModeCondition</name>  
      <location>com.icss.ro.framework.login.chain.condition.LoginModeCondition</location>  
      <processType>1</processType>  
      <nodeType>2</nodeType>  
      <priority>8</priority>  
      <enable>true</enable> 
    </LoginModule>  
    <LoginModule> 
      <name>SSOCondition</name>  
      <location>com.icss.ro.framework.login.chain.condition.SSOCondition</location>  
      <processType>1</processType>  
      <nodeType>2</nodeType>  
      <priority>7</priority>  
      <enable>true</enable>  
      <depend>OpenPortalCondition</depend> 
    </LoginModule>  
    <LoginModule> 
      <name>AutoLoginCondition</name>  
      <location>com.icss.ro.framework.login.chain.condition.AutoLoginCondition</location>  
      <processType>1</processType>  
      <nodeType>2</nodeType>  
      <priority>7</priority>  
      <enable>true</enable>  
      <depend>OpenPortalCondition</depend> 
    </LoginModule>  
    <LoginModule> 
      <name>DomainCondition</name>  
      <location>com.icss.ro.framework.login.chain.condition.DomainCondition</location>  
      <processType>1</processType>  
      <nodeType>2</nodeType>  
      <priority>5</priority>  
      <enable>true</enable>  
      <depend>SingleLoginCondition</depend> 
    </LoginModule> 
  </LoginModules>  
  <Processors> 
    <Processor> 
      <name>AuthenProcessor</name>  
      <subModule> 
        <moduleName>LoginModeCondition</moduleName>  
        <result> 
          <type>boolean</type>  
          <transition> 
            <value>false</value>  
            <nextModule/> 
          </transition>  
          <transition> 
            <value>true</value>  
            <nextModule>BasicAuthenAcivity</nextModule> 
          </transition> 
        </result> 
      </subModule> 
    </Processor>  
    <Processor> 
      <name>UserMenuOnceProcessor</name>  
      <subModule> 
        <moduleName>GetSystemActivity</moduleName>  
        <nextModule>GetApplictionsActivity</nextModule> 
      </subModule>  
      <subModule> 
        <moduleName>GetApplictionsActivity</moduleName>  
        <nextModule>GetMenuActivity</nextModule> 
      </subModule> 
    </Processor> 
  </Processors>  
  <LoginChain> 
    <LoginModule> 
      <moduleName>AuthenProcessor</moduleName>  
      <nextModule>UserMenuOnceProcessor</nextModule> 
    </LoginModule>  
    <LoginModule> 
      <moduleName>UserMenuOnceProcessor</moduleName> 
    </LoginModule> 
  </LoginChain> 
</LoginConfig>
