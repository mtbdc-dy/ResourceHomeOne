<?xml version="1.0" encoding="UTF-8"?>

<SystemConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="SystemConfig.xsd">  
  <Update> 
    <HasUpdated>false</HasUpdated>  
    <UpdateTime/> 
  </Update>  
  <!--ResourceOne 产品授权信息-->  
  <VersionConfig> 
    <VersionNum>4.2</VersionNum>  
    <VersionDesc>ResourceOne Enterprise Extended Edition</VersionDesc>  
    <licensePath>{resourceOneHome}/license/rone.lic</licensePath> 
  </VersionConfig>  
  <!--分布式部署配置-->  
  <DistributeConfig> 
    <LocalConfig> 
      <IsNodeServer>true</IsNodeServer>  
      <NodeName>R1Framework4.0</NodeName>  
      <NodeUUID>R1Framework4.0</NodeUUID>  
      <Name/> 
    </LocalConfig>  
    <ServerConfig> 
      <Host>Text</Host>  
      <Port>0</Port> 
    </ServerConfig>  
    <ScanBeforeList>false</ScanBeforeList> 
  </DistributeConfig>  
  <!--授权及安全配置-->  
  <AuthenticateConfig> 
    <!--多种授权配置-->  
    <ValidateConfig> 
      <ValidateType>String</ValidateType>  
      <KeyFile>Text</KeyFile>  
      <UserName>String</UserName>  
      <Password>String</Password> 
    </ValidateConfig>  
    <!--单点登录配置-->  
    <SSOConfig> 
      <Enable>true</Enable>  
      <SSOServerSite>http://localhost</SSOServerSite>  
      <SSOSecureEnable>false</SSOSecureEnable>  
      <SSOServerURL>/servlet/SSOServer</SSOServerURL>  
      <SSOLoginURL>/resourceone/sso/Login.jsp</SSOLoginURL>  
      <LoginHandlerClassName>com.icss.resourceone.sdk.SdkSignOn</LoginHandlerClassName>  
      <ValidateMode>DataBase</ValidateMode>  
      <SSOLoginSuccessPage>Text</SSOLoginSuccessPage>  
      <SSOLoginFailurePage>/index.jsp</SSOLoginFailurePage>  
      <IsShowLoginSucess>Text</IsShowLoginSucess>  
      <IsShowLoginFailure>Text</IsShowLoginFailure>  
      <TimeOut>-1</TimeOut>  
      <SSOWithAppSrv>false</SSOWithAppSrv>  
      <NoServerAuthen>false</NoServerAuthen>  
      <EncryptLTPA>false</EncryptLTPA>  
      <EncryptLTPAImplClass/> 
    </SSOConfig>  
    <!--加解密算法配置-->  
    <EncryptConfig> 
      <ConfigFilePath>{resourceOneHome}</ConfigFilePath> 
    </EncryptConfig>  
    <!--日志系统配置-->  
    <LogConfig> 
      <LogImplClassName>com.icss.j2ee.log.impl.LogFactoryImpl</LogImplClassName>  
      <ConfigFilePath>{resourceOneHome}</ConfigFilePath> 
    </LogConfig>  
    <!--行为审核配置-->  
    <AuditConfig> 
      <ConfigFilePath>{resourceOneHome}</ConfigFilePath> 
    </AuditConfig>  
    <!--权限控制配置-->  
    <RightConfig> 
      <ActionRights> 
        <Create>Text</Create>  
        <Modify>Text</Modify>  
        <Read>Text</Read>  
        <Delete>Text</Delete>  
        <Approve>Text</Approve> 
      </ActionRights>  
      <ActionEnable>true</ActionEnable>  
      <IsGlobalControl>true</IsGlobalControl>  
      <IsShowDenyChannel>false</IsShowDenyChannel>
	  <ResDistribute>true</ResDistribute>
    </RightConfig> 
  </AuthenticateConfig>  
  <GUIPublishConfig> 
    <GUISystem> 
      <Name>Text</Name>  
      <PublishClassName>Text</PublishClassName>  
      <DataSourceJNDI>Text</DataSourceJNDI>  
      <UserName>Text</UserName>  
      <Password>Text</Password>  
      <LastPublishTime>Text</LastPublishTime>  
      <LastPublishPersonUUID>Text</LastPublishPersonUUID> 
    </GUISystem> 
  </GUIPublishConfig>  
  <!--目录服务配置-->  
  <DirectoryConfig> 
    <Enable>false</Enable>  
    <Host>192.9.100.14</Host>  
    <Port>389</Port>  
    <UserName>administrator</UserName>  
    <Password>icsspass</Password>  
    <ManagerClassName>com.icss.resourceone.sdkimpl.directory.ldapimpl.LdapDirectoryManager</ManagerClassName>  
    <mappingImplClassName>com.icss.resourceone.configuration.ldapamapping.XMLConfigImpl</mappingImplClassName>  
    <BindDN>dc=rone,dc=com</BindDN>  
    <IsUpdateFromLocal>false</IsUpdateFromLocal>  
    <ServerType>MSAD</ServerType>  
    <PasswdSetSchema>SSL</PasswdSetSchema>  
    <KeyStore>C:/cacerts.keystore</KeyStore>  
    <TrustStorePasswd>changeit</TrustStorePasswd> 
  </DirectoryConfig>  
  <!--应用框架管理配置-->  
  <AppFrameworkConfig> 
    <EntityManagerClassName>com.icss.resourceone.sdkimpl.framework.DBEntityManager</EntityManagerClassName> 
  </AppFrameworkConfig>  
  <!--数据传输通道配置-->  
  <DataChannelConfig> 
    <ConfigFilePath>Text</ConfigFilePath>  
    <GloblaConfigStore> 
      <store>file</store> 
    </GloblaConfigStore>  
    <LocalConfigSource>file</LocalConfigSource>  
    <CurrentJmsNodeName>Text</CurrentJmsNodeName>  
    <CurrentJmsFactory>Text</CurrentJmsFactory> 
  </DataChannelConfig>  
  <!--软件管理配置-->  
  <SoftwareManagerConfig> 
    <softwareConfig> 
      <UUID>Text</UUID>  
      <Name>Text</Name>  
      <Version>Text</Version>  
      <Type>Text</Type>  
      <Root>Text</Root>  
      <WarName>Text</WarName>  
      <InstallDate>Text</InstallDate>  
      <InstallPersonUUID>Text</InstallPersonUUID>  
      <Dependency> 
        <SoftwareUUID>Text</SoftwareUUID>  
        <VersionNum/> 
      </Dependency> 
    </softwareConfig> 
  </SoftwareManagerConfig>  
  <!--系统初始化程序配置-->  
  <InitConfig> 
    <Initlet> 
      <InitName>PersonInitlet</InitName>  
      <ClassName>com.icss.resourceone.init.PersonInitlet</ClassName>  
      <!--当不需要LDAP时,可设为false -->  
      <Parameter> 
        <Name>MigrateEnable</Name>  
        <Value>false</Value> 
      </Parameter>  
      <!--  &&&&&&&&&人员信息扩展字段的扩展实现类注册如下&&&&&&& --> 
    </Initlet>  
    <Initlet> 
      <InitName>SystemMutilTableInit</InitName>  
      <ClassName>com.icss.resourceone.init.SystemMutilTableInit</ClassName> 
    </Initlet>  
    <Initlet> 
      <InitName>SubSystemDeployInitlet</InitName>  
      <ClassName>com.icss.resourceone.init.SubSystemDeployInitlet</ClassName>  
      <!-- &&&&&&&&&&&&& 下面的参数设置子系统的部署节点下拉参数列表 &&&&&&&&&&&&& -->  
      <Parameter> 
        <Name>DeployNodeCodeSet</Name>  
        <Value>deploy1</Value> 
      </Parameter>  
      <Parameter> 
        <Name>DeployNodeCodeSet</Name>  
        <Value>deploy2</Value> 
      </Parameter>  
      <!-- &&&&&&&&&&&&& 下面的参数设置该部署节点能够访问的子系统部署标识码 &&&&&&&&&&&&& -->  
      <Parameter> 
        <Name>LimitDeployNodeCode</Name>  
        <Value>deploy1</Value> 
      </Parameter> 
    </Initlet>  
    <Initlet> 
      <InitName>R1ExtensionInitlet</InitName>  
      <ClassName>com.icss.resourceone.init.R1ExtensionInitlet</ClassName>  
      <!--
			<Parameter>
				<Name>UserManageClassName</Name>
				<Value>com.icss.resourceone.sdk.test.TestUserExtendProcessor</Value>
			</Parameter>-->  
      <!-- 组织管理扩展处理类注册
			<Parameter>
				<Name>OrgManageClassName</Name>
				<Value>com.icss.resourceone.sdk.test.TestOrgExtendProcessor</Value>
			</Parameter>-->  
      <!--
			<Parameter>
				<Name>ActionRecordEnable</Name>
				<Value>true</Value>
			</Parameter>
			<Parameter>
				<Name>RecordImplClass</Name>
				<Value>com.icss.resourceone.actionrecord.handler.ActionRecorderDeftImpl</Value>
			</Parameter>
			<Parameter>
				<Name>RecordCacheSize</Name>
				<Value>100</Value>
			</Parameter>
			--> 
    </Initlet>  
    <Initlet> 
      <InitName>OnlineUsersInitlet</InitName>  
      <ClassName>com.icss.resourceone.init.OnlineUsersInitlet</ClassName> 
    </Initlet> 
  </InitConfig>  
  <!--系统登录登出配置-->  
  <LogInOutConfig> 
    <LogInConfig> 
      <MaxLoginFailNum>3</MaxLoginFailNum>  
      <AccoutTTL>0</AccoutTTL>  
      <ErrorPage>/SignOnServlet</ErrorPage>  
      <LoginOkToPage>/portal</LoginOkToPage>  
      <LoginMode>UserID/Password</LoginMode>  
      <!--UserID/Password  CA   mix-->  
      <loginHandlerClassName>com.icss.resourceone.common.login.RoLoginHandler</loginHandlerClassName>  
      <loginCAImplClassName>com.icss.resourceone.common.login.ca.impl.TobaccoCAValidate</loginCAImplClassName>  
      <loginUrl>/login</loginUrl>  
      <logoutUrl>/logout</logoutUrl>  
      <notLoginPage>/SignOnServlet</notLoginPage>  
      <loginFailurePage>/SignOnServlet</loginFailurePage>  
      <loginSuccessPage>/SignOnServlet</loginSuccessPage>  
      <loginTryPage>/SignOnServlet</loginTryPage>  
      <loginCuePage>/SignOnServlet</loginCuePage>  
      <showLoginSuccess>false</showLoginSuccess>  
      <showLoginFailure>false</showLoginFailure>  
      <defaultLoginPage/>  
      <portalHomePage>http://portal</portalHomePage>  
      <singleLoginEnable>false</singleLoginEnable>  
      <!--single|cluster-->  
      <LoginServer>single</LoginServer>  
      <concurrentThreshold>10</concurrentThreshold>  
      <waitInterval>100</waitInterval>  
      <UserLockedModelCompatible>false</UserLockedModelCompatible> 
    </LogInConfig>  
    <LoginOption> 
      <loginNumControlEnable>false</loginNumControlEnable>  
      <conCurrentControlEnable>false</conCurrentControlEnable>  
      <isOpenPortal>false</isOpenPortal>  
      <autoLoginEnable>false</autoLoginEnable>  
      <allownDomainAutoLogin>false</allownDomainAutoLogin>  
      <securePolicyEnable>false</securePolicyEnable>  
      <jaasEnable>false</jaasEnable>  
      <customizationEnable>true</customizationEnable>  
      <displayByStep>false</displayByStep>  
      <systemCacheEnable>true</systemCacheEnable>  
      <appCacheEnable>false</appCacheEnable>  
      <profileCacheEnable>true</profileCacheEnable>  
      <gobalSystemEnable>true</gobalSystemEnable>  
      <gobalSystemGuestEnable>true</gobalSystemGuestEnable>  
      <usedVerifyCode>false</usedVerifyCode>  
      <adminHasNormalRight>false</adminHasNormalRight> 
    </LoginOption>  
    <LogOutConfig> 
      <LogOutPage>${R1CONTEXT}/logout</LogOutPage> 
    </LogOutConfig> 
  </LogInOutConfig>  
  <!--系统框架的一些基础配置-->  
  <BaseConfig> 
    <!--系统消息显示设置-->  
    <MessageConfig> 
      <commonPage/>  
      <warningPage/> 
    </MessageConfig>  
    <!--权限出错显示设置-->  
    <Right_BaseConfig> 
      <noRightPage/> 
    </Right_BaseConfig>  
    <!--上传路径设置-->  
    <UploadConfig> 
      <fileBaseUrl>{resourceOneHome}/upload</fileBaseUrl>  
      <tempPath>{resourceOneHome}/upload/temp</tempPath>  
      <uploadEncode>utf-8</uploadEncode> 
    </UploadConfig> 
  </BaseConfig>  
  <Plugin> 
    <Extensions/> 
  </Plugin>  
  <R1ClientConfig> 
    <SdkDataSourceJndi>ResourceOne/DataSource</SdkDataSourceJndi>  
    <!-- pool:用appserver的datasource    jdbc:使用普通jdbc连接 -->  
    <SdkConnectionType>pool</SdkConnectionType>  
    <JdbcDriver>COM.ibm.db2.jdbc.app.DB2Driver</JdbcDriver>  
    <Url>jdbc:db2:RONE</Url>  
    <User>db2admin</User>  
    <Password>db2admin</Password>  
    <GetUserDetailInfoWhenLogin>true</GetUserDetailInfoWhenLogin>  
    <BeListened/> 
  </R1ClientConfig>  
  <!-- Portal 服务 -->  
  <PortalServerConfig> 
    <!--  是否启动Portlet Container -->  
    <StartPortletContainer>true</StartPortletContainer>  
    <AutoDeployDefaultDestDir>D:\tomcat5.5\webapps</AutoDeployDefaultDestDir>  
    <DBDialect>org.hibernate.dialect.DB2Dialect</DBDialect>  
    <Ctx>/rone</Ctx> 
  </PortalServerConfig>  
  <LocaleConfig> 
    <SystemLocale>zh_CN</SystemLocale> 
  </LocaleConfig>  
  <AppearanceConfig> 
    <SiteTitle/>  
    <TopSystemName/>  
    <TopOrgName/>  
    <NoSrcNavShow/>  
    <NavbarWidth/> 
  </AppearanceConfig> 
</SystemConfig>
